{
  "name": "urbanclimbing",
  "version": "0.1.1",
  "author": {
    "name": "Sara Gfroerer"
  },
  "description": "Urbanclimbing, your interactive guide to your next climbing adventure in the city. Urbanclimbing is a capstone project",
  "private": true,
  "dependencies": {
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "cypress": "^3.7.0",
    "eslint-plugin-cypress": "^2.7.0",
    "axios": "^0.21.1",
    "cloudinary-react": "^1.2.1",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "mongodb": "^3.4.1",
    "mongoose": "^5.8.7",
    "nodemon": "^2.0.1",
    "now": "^16.7.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-maps": "^9.4.5",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-last-location": "^2.0.1",
    "react-scripts": "3.2.0",
    "react-spring": "^8.0.27",
    "save": "^2.4.0",
    "storybook-addon-jsx": "^7.1.13",
    "storybook-router-react": "^0.3.3",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "node server/server.js",
    "dev": "run-p client server",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "now --public",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "cypress:open": "cypress open",
    "server": "PORT=3333 nodemon --inspect --watch ./server ./server/server.js"
  },
  "proxy": "http://localhost:3333",
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "now": {
    "name": "urbanclimbing",
    "version": 2,
    "env": {
      "MONGODB_URI": "@my-mongodb-uri"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6"
  }
}
